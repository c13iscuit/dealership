<h1>Cars Index</h1>

<p><%= link_to 'See all manufacturers', manufacturers_path%></p>
<p><%= link_to 'Add a car!', new_car_path %></p>

<ul>
<% @cars.each do |car| %>
  <li><%= "#{car.year} #{car.manufacturer.name} - #{car.color}, #{car.mileage} miles" %>
  <p> Description: <%= car.description %></p></li>
<% end %>
